package com.company;

class A{
    public void meth(){
        System.out.println("i am method1 of class A");
    }
}

class B extends A{
    //method overriding
    public void meth(){
        System.out.println(" I am method 1 of class B");
    }

}
public class HMethodOverirde {
    public static void main(String[] args) {
        A a = new A();
        a.meth();

        B b = new B();
        b.meth();
    }
}
